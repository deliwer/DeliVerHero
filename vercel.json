{
  "buildCommand": "npx vite build --outDir=dist/public",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/shopify/(.*)",
      "destination": "/api/shopify.js"
    }
  ]
}