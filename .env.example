# Shopify Horizon Configuration
SHOPIFY_API_KEY=your_shopify_api_key_here
SHOPIFY_API_SECRET=your_shopify_api_secret_here
SHOPIFY_SCOPES=write_products,read_products,write_orders,read_orders,write_customers,read_customers,write_draft_orders,read_draft_orders,write_inventory,read_inventory,write_fulfillments,read_fulfillments
SHOPIFY_WEBHOOK_SECRET=your_webhook_secret_here

# DeliWer Environment
NODE_ENV=production
VITE_API_URL=https://deliwer-ecosystem.vercel.app/api
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration (Optional - for future PostgreSQL integration)
DATABASE_URL=your_database_url_here
PGHOST=your_db_host
PGPORT=5432
PGUSER=your_db_user
PGPASSWORD=your_db_password
PGDATABASE=deliwer_production

# Vercel Environment Variables
VERCEL_URL=deliwer-ecosystem.vercel.app
VERCEL_DOMAIN=deliwer-ecosystem.vercel.app